	<div class = "container">
		<div class="text-center">
		<h1>Your Perfect Job Match Below!</h1>

		<hr class="rule">
		<a href="/search"><button class="btn btn-success">Back to Search</button></a>
		<hr class="rule">
		</div>
		
			<div class="space">
				{{#each posting}}

			<div class="panel panel-primary shadow">
  				<div class="panel-heading">
   			 	<h3 class="panel-title">{{this.title}}</h3>   				 
 			 	</div>
 			<div class="panel-body">
    			<h4>{{this.company}}</h4>
    			<h5>{{this.location}}</h5>
    			<br/>
    			<p> {{this.summary}}</p>
    			<a href ={{this.url}} class="btn btn-success" target="_blank"><span class="glyphicon glyphicon-search"></span></a>
    			<button class="btn btn-primary profileBtn" value="{{this.company}}" >View Company Profile</button>
  			</div>
		</div>
			
		{{/each}}
		{{#each company}}
		<h1>{{this.companyName}}</h1>
		{{/each}}
		</div>
		
	</div>	

<script type="text/javascript" src = "/javascript/results.js"></script>